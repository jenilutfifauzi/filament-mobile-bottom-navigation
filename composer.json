{
    "name": "jenilutfifauzi/filament-mobile-bottom-navigation",
    "description": "Mobile bottom navigation bar plugin for Filament admin panels",
    "keywords": [
        "jenilutfifauzi",
        "laravel",
        "filament-mobile-bottom-navigation"
    ],
    "homepage": "https://github.com/jenilutfifauzi/filament-mobile-bottom-navigation",
    "support": {
        "issues": "https://github.com/jenilutfifauzi/filament-mobile-bottom-navigation/issues",
        "source": "https://github.com/jenilutfifauzi/filament-mobile-bottom-navigation"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jeni Lutfi Fauzi",
            "email": "jenilutfifauzi18@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0|^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Jenilutfifauzi\\FilamentMobileBottomNavigation\\": "src/",
            "Jenilutfifauzi\\FilamentMobileBottomNavigation\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenilutfifauzi\\FilamentMobileBottomNavigation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jenilutfifauzi\\FilamentMobileBottomNavigation\\FilamentMobileBottomNavigationServiceProvider"
            ],
            "aliases": {
                "FilamentMobileBottomNavigation": "Jenilutfifauzi\\FilamentMobileBottomNavigation\\Facades\\FilamentMobileBottomNavigation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
